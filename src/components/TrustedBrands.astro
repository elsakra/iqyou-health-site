---
// Trusted Brands component
---

<section class="section-spacing bg-brand-cream">
  <div class="container-custom">
    <!-- Section Header -->
    <div class="text-center mb-16">
      <h2 class="text-4xl lg:text-5xl font-serif text-brand-dark mb-6">
        Trusted by 
        <span class="italic text-brand-orange">Leading Brands</span>
      </h2>
      <p class="text-xl text-gray-600 max-w-3xl mx-auto leading-relaxed">
        We partner with the most respected names in nutritional supplements and wellness to bring you premium quality products.
      </p>
    </div>

    <!-- Brands Grid -->
    <div class="grid grid-cols-2 md:grid-cols-4 lg:grid-cols-8 gap-8 items-center mb-12">
      <div class="bg-white rounded-xl p-6 shadow-md hover:shadow-lg transition-shadow duration-300 group">
        <img 
          src="https://res.cloudinary.com/dil14r8je/image/upload/v1768288504/auto-site-builder/IQYOU%20Health/image_32.jpg" 
          alt="Pathway Essentials" 
          class="w-full h-16 object-contain group-hover:scale-105 transition-transform duration-300"
        />
      </div>
      
      <div class="bg-white rounded-xl p-6 shadow-md hover:shadow-lg transition-shadow duration-300 group">
        <img 
          src="https://res.cloudinary.com/dil14r8je/image/upload/v1768288504/auto-site-builder/IQYOU%20Health/image_33.jpg" 
          alt="Thorne" 
          class="w-full h-16 object-contain group-hover:scale-105 transition-transform duration-300"
        />
      </div>
      
      <div class="bg-white rounded-xl p-6 shadow-md hover:shadow-lg transition-shadow duration-300 group">
        <img 
          src="https://res.cloudinary.com/dil14r8je/image/upload/v1768288504/auto-site-builder/IQYOU%20Health/image_34.jpg" 
          alt="Pure Encapsulations" 
          class="w-full h-16 object-contain group-hover:scale-105 transition-transform duration-300"
        />
      </div>
      
      <div class="bg-white rounded-xl p-6 shadow-md hover:shadow-lg transition-shadow duration-300 group">
        <img 
          src="https://res.cloudinary.com/dil14r8je/image/upload/v1768288506/auto-site-builder/IQYOU%20Health/image_35.jpg" 
          alt="Metagenics" 
          class="w-full h-16 object-contain group-hover:scale-105 transition-transform duration-300"
        />
      </div>
      
      <div class="bg-white rounded-xl p-6 shadow-md hover:shadow-lg transition-shadow duration-300 group">
        <img 
          src="https://res.cloudinary.com/dil14r8je/image/upload/v1768288506/auto-site-builder/IQYOU%20Health/image_36.jpg" 
          alt="Nordic Naturals" 
          class="w-full h-16 object-contain group-hover:scale-105 transition-transform duration-300"
        />
      </div>
      
      <div class="bg-white rounded-xl p-6 shadow-md hover:shadow-lg transition-shadow duration-300 group">
        <img 
          src="https://res.cloudinary.com/dil14r8je/image/upload/v1768288506/auto-site-builder/IQYOU%20Health/image_37.png" 
          alt="XyMogen" 
          class="w-full h-16 object-contain group-hover:scale-105 transition-transform duration-300"
        />
      </div>
      
      <div class="bg-white rounded-xl p-6 shadow-md hover:shadow-lg transition-shadow duration-300 group">
        <img 
          src="https://res.cloudinary.com/dil14r8je/image/upload/v1768288506/auto-site-builder/IQYOU%20Health/image_38.jpg" 
          alt="Researched Nutritionals" 
          class="w-full h-16 object-contain group-hover:scale-105 transition-transform duration-300"
        />
      </div>
      
      <div class="bg-white rounded-xl p-6 shadow-md hover:shadow-lg transition-shadow duration-300 group">
        <img 
          src="https://res.cloudinary.com/dil14r8je/image/upload/v1768288506/auto-site-builder/IQYOU%20Health/image_39.jpg" 
          alt="Klaire Labs" 
          class="w-full h-16 object-contain group-hover:scale-105 transition-transform duration-300"
        />
      </div>
    </div>

    <!-- Brand Categories -->
    <div class="grid md:grid-cols-3 gap-8">
      <!-- Practitioner Brands -->
      <div class="card text-center">
        <h3 class="text-xl font-serif text-brand-dark mb-4">Practitioner Brands</h3>
        <p class="text-gray-600 text-sm mb-6">
          Professional-grade supplements trusted by healthcare practitioners worldwide.
        </p>
        <div class="space-y-2 text-sm text-gray-600">
          <p>• Thorne Research</p>
          <p>• Pure Encapsulations</p>
          <p>• Metagenics</p>
          <p>• Researched Nutritionals</p>
        </div>
      </div>

      <!-- Consumer Brands -->
      <div class="card text-center">
        <h3 class="text-xl font-serif text-brand-dark mb-4">Top Consumer Brands</h3>
        <p class="text-gray-600 text-sm mb-6">
          High-quality supplements for everyday wellness and optimal health.
        </p>
        <div class="space-y-2 text-sm text-gray-600">
          <p>• Nordic Naturals</p>
          <p>• Life Extension</p>
          <p>• Garden of Life</p>
          <p>• Solgar</p>
        </div>
      </div>

      <!-- Specialty Brands -->
      <div class="card text-center">
        <h3 class="text-xl font-serif text-brand-dark mb-4">Specialty Solutions</h3>
        <p class="text-gray-600 text-sm mb-6">
          Targeted formulations for specific health concerns and conditions.
        </p>
        <div class="space-y-2 text-sm text-gray-600">
          <p>• Microbiome Labs</p>
          <p>• Quicksilver Scientific</p>
          <p>• Host Defense</p>
          <p>• Pathway Essentials</p>
        </div>
      </div>
    </div>

    <!-- CTA Section -->
    <div class="text-center mt-12">
      <button class="btn-primary text-lg px-12 py-4">
        View All Brands
      </button>
    </div>
  </div>
</section>